{
    "version": 2,
      "routes": [
        {
          "src": "/api/user",
          "dest": "/routes/user_routes.js"
        }
      ],
      "rewrites": [
        {
          "source": "/(.*)",
          "destination": "/$1"
        }
      ],
      "builds": [
        {
          "src": "/config/Database.js",
          "use": "@now/node"
        },
        {
          "src": "/controllers/User_Controller.js",
          "use": "@now/node"
        },
        {
          "src": "/models/user_models.js",
          "use": "@now/node"
        }
      ]
}